#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ivan/development/graphql-eslint-test/node_modules/.pnpm/ts-node-dev@1.1.8_typescript@4.5.4/node_modules/ts-node-dev/lib/node_modules:/home/ivan/development/graphql-eslint-test/node_modules/.pnpm/ts-node-dev@1.1.8_typescript@4.5.4/node_modules/ts-node-dev/node_modules:/home/ivan/development/graphql-eslint-test/node_modules/.pnpm/ts-node-dev@1.1.8_typescript@4.5.4/node_modules:/home/ivan/development/graphql-eslint-test/node_modules/.pnpm/node_modules:/home/ivan/development/graphql-eslint-test/node_modules:/home/ivan/development/node_modules:/home/ivan/node_modules:/home/node_modules:/node_modules:/home/ivan/development/graphql-eslint-test/server/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ivan/development/graphql-eslint-test/node_modules/.pnpm/ts-node-dev@1.1.8_typescript@4.5.4/node_modules/ts-node-dev/lib/node_modules:/home/ivan/development/graphql-eslint-test/node_modules/.pnpm/ts-node-dev@1.1.8_typescript@4.5.4/node_modules/ts-node-dev/node_modules:/home/ivan/development/graphql-eslint-test/node_modules/.pnpm/ts-node-dev@1.1.8_typescript@4.5.4/node_modules:/home/ivan/development/graphql-eslint-test/node_modules/.pnpm/node_modules:/home/ivan/development/graphql-eslint-test/node_modules:/home/ivan/development/node_modules:/home/ivan/node_modules:/home/node_modules:/node_modules:/home/ivan/development/graphql-eslint-test/server/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/ts-node-dev@1.1.8_typescript@4.5.4/node_modules/ts-node-dev/lib/bin.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/ts-node-dev@1.1.8_typescript@4.5.4/node_modules/ts-node-dev/lib/bin.js" "$@"
fi
