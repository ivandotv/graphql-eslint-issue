#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ivan/development/graphql-eslint-test/node_modules/.pnpm/shx@0.3.3/node_modules/shx/lib/node_modules:/home/ivan/development/graphql-eslint-test/node_modules/.pnpm/shx@0.3.3/node_modules/shx/node_modules:/home/ivan/development/graphql-eslint-test/node_modules/.pnpm/shx@0.3.3/node_modules:/home/ivan/development/graphql-eslint-test/node_modules/.pnpm/node_modules:/home/ivan/development/graphql-eslint-test/node_modules:/home/ivan/development/node_modules:/home/ivan/node_modules:/home/node_modules:/node_modules:/home/ivan/development/graphql-eslint-test/server/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ivan/development/graphql-eslint-test/node_modules/.pnpm/shx@0.3.3/node_modules/shx/lib/node_modules:/home/ivan/development/graphql-eslint-test/node_modules/.pnpm/shx@0.3.3/node_modules/shx/node_modules:/home/ivan/development/graphql-eslint-test/node_modules/.pnpm/shx@0.3.3/node_modules:/home/ivan/development/graphql-eslint-test/node_modules/.pnpm/node_modules:/home/ivan/development/graphql-eslint-test/node_modules:/home/ivan/development/node_modules:/home/ivan/node_modules:/home/node_modules:/node_modules:/home/ivan/development/graphql-eslint-test/server/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/shx@0.3.3/node_modules/shx/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/shx@0.3.3/node_modules/shx/lib/cli.js" "$@"
fi
